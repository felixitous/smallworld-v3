<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="smallworld.smallworld_v3" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SimpleProfile"
            android:label="@string/title_activity_simple_profile" >
        </activity>
        <activity
            android:name=".UserWithDossier"
            android:label="@string/title_activity_user_with_dossier" >
        </activity>
        <activity
            android:name=".ComplicatedProfile"
            android:label="@string/title_activity_complicated_profile" >
        </activity>
    </application>

</manifest>
